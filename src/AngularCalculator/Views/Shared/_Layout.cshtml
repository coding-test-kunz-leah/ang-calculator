<!DOCTYPE html>

<html>
<head>
    <base href="/">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <!--1. Configuring angular related libraries-->
    <script src="~/libraries/core-js/client/shim.min.js"></script>
    <script src="~/libraries/zone.js/dist/zone.js"></script>
    <script src="~/libraries/systemjs/dist/system.src.js"></script>
    <link href="~/libraries/&#64;angular/material/core/theming/prebuilt/indigo-pink.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
          rel="stylesheet">

    <!-- 2. Configure SystemJS -->
    <script src="~/js/Systemjs.config.js"></script>

    <!-- 3. link custom css -->
    <link href="~/css/calstyle.css" rel="stylesheet">


    <script>
        System.import('tsjs/main').catch(function (err) {
            console.error('the-error-is: ' + err);
        });
    </script>
</head>
<body>

   
    <div>
        @RenderBody()
    </div>

    
</body>
</html>
